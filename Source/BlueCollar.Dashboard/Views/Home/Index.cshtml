@model HomeIndex

@section script {
<script type="text/javascript" src="@Url.Content("~/Static/Js/collar.js")"></script>
<script type="text/javascript">
    var app;
    $(function() {
        app = new App('@BlueCollarSection.Section.ApplicationName', '@Url.Content("~/")', {
            chartsLoaded: false,
            counts: @Html.Raw(Model.CountsJson),
            jsonUrlRoot: '@Url.Content(BlueCollarSection.Section.Dashboard.HandlerUrl)',
            testLink: true
        });
    });
</script>
<script type="text/javascript" src="//www.google.com/jsapi?autoload={'modules':[{'name':'visualization','version':'1','packages':['corechart']}]}"></script>
<script type="text/javascript">
    google.setOnLoadCallback(function() {
        app.setChartsLoaded(true); 
    });
</script>
}